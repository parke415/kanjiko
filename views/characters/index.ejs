<%- include('../partials/header') %>
  <h1><%= title %><br>(<span <%- learnedChars === characters.length && learnedChars !== 0 ? 'style="color: #00bbbb;"' : 'style="color: #bb0000;"' %>><%= learnedChars %>/<%= characters.length %> LEARNED</span>)</h1>
  <br>
  <% characters.sort((a, b) => a.strokes - b.strokes); %>
  <% characters.forEach(character => { %>
  <div class="char" <%- character.learned ? 'style="border-color: #337777; background-color: #77bbbb;"' : 'style="border-color: #773333; background-color: #bb7777;"' %>><a href="/characters/<%= character.id %>"><%= character.glyph %></a></div>
  <% }); %>
  <div class="char"><a href="/characters/new" style="color: #777777">ï¼‹</a></div>
<%- include('../partials/footer') %>